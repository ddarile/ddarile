            </div>
            <div class="modal-body">
                <form class="admin-form" id="editServiceForm" action="/admin/service/update" method="POST" enctype="multipart/form-data">
                    <input type="hidden" id="editServiceId" name="serviceId">
                    <div class="form-row">
                      <div class="form-group">
                        <label for="editServiceName">Название услуги</label>
                        <input type="text" id="editServiceName" name="serviceName" required>
                      </div>
                      <div class="form-group">
                        <label for="editServiceCategory">Категория</label>
                        <select id="editServiceCategory" name="serviceCategory" required>
                          <% designTypes.forEach((type) => { %>
                            <option value="<%= type.id_design_type %>"><%= type.design_type_name %></option>
                          <% }); %>
                        </select>
                      </div>
                    </div>
                
                    <div class="form-group">
                      <label for="editServiceCover">Обложка услуги</label>
                      <div class="current-cover-preview">
                        <img id="editServiceCoverPreview" src="/placeholder.svg" alt="Текущая обложка"
                          style="max-width: 200px; max-height: 150px; border-radius: 8px; margin-bottom: 10px;">
                      </div>
                      <input type="file" id="editServiceCover" name="serviceCover" accept="image/*">
                    </div>
                
                    <div class="form-group">
                      <label for="editServiceDescription">Описание услуги</label>
                      <textarea id="editServiceDescription" name="serviceDescription" rows="4" required></textarea>
                    </div>
                
                    <div class="form-group">
                      <label for="editServicePriceFrom">Цена от (₽)</label>
                      <input type="number" id="editServicePriceFrom" name="servicePriceFrom" min="0" required>
                    </div>
                    <div class="form-actions-center">
                        <button type="submit" class="btn btn-primary">
                            <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor"
                                stroke-width="2">
                                <path d="M19 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h11l5 5v11a2 2 0 0 1-2 2z"></path>
                                <polyline points="17,21 17,13 7,13 7,21"></polyline>
                                <polyline points="7,3 7,8 15,8"></polyline>
                            </svg>
                            Сохранить изменения
                        </button>
                        <button type="button" class="btn btn-secondary" data-modal="editServiceModal">
                            <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor"
                                stroke-width="2">
                                <line x1="18" y1="6" x2="6" y2="18"></line>
                                <line x1="6" y1="6" x2="18" y2="18"></line>
                            </svg>
                            Отмена
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <!-- Модальное окно редактирования типа продукции -->
    <div id="editProductTypeModal" class="modal">
        <div class="modal-content large">
            <div class="modal-header">
                <h2>Редактирование типа продукции</h2>
                <span class="close" data-modal="editProductTypeModal">&times;</span>
            </div>
            <div class="modal-body">
                <form class="admin-form" id="editProductTypeForm">
                    <input type="hidden" id="editProductTypeId" name="productTypeId">
                    <div class="form-row">
                        <div class="form-group">
                            <label for="editProductTypeName">Название типа продукции</label>
                            <input type="text" id="editProductTypeName" name="productTypeName" required>
                        </div>
                        <div class="form-group">
                            <label for="editProductTypeService">Услуга</label>
                            <select id="editProductTypeService" name="productTypeService" required>
                                <% services.forEach((service) => { %>
                                  <option value="<%= service.id_service %>">
                                    <%= service.service_name %>
                                  </option>
                                <% }); %> 
                            </select>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group">
                            <label for="editProductTypePrice">Цена (₽)</label>
                            <input type="number" id="editProductTypePrice" name="productTypePrice" min="0" required>
                        </div>
                        <div class="form-group">
                            <label for="editProductTypeDuration">Срок реализации</label>
                            <input type="text" id="editProductTypeDuration" name="productTypeDuration"
                                placeholder="например: 3 дня" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="editProductTypeDescription">Описание</label>
                        <textarea id="editProductTypeDescription" name="productTypeDescription" rows="4"
                            required></textarea>
                    </div>
                    <div class="form-actions-center">
                        <button type="submit" class="btn btn-primary">
                            <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor"
                                stroke-width="2">
                                <path d="M19 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h11l5 5v11a2 2 0 0 1-2 2z"></path>
                                <polyline points="17,21 17,13 7,13 7,21"></polyline>
                                <polyline points="7,3 7,8 15,8"></polyline>
                            </svg>
                            Сохранить изменения
                        </button>
                        <button type="button" class="btn btn-secondary" data-modal="editProductTypeModal">
                            <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor"
                                stroke-width="2">
                                <line x1="18" y1="6" x2="6" y2="18"></line>
                                <line x1="6" y1="6" x2="18" y2="18"></line>
                            </svg>
                            Отмена
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <script src="/scripts/menu_script.js"></script>
    <script src="/scripts/admin_script.js"></script>
</body>

</html>